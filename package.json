{
  "private": true,
  "name": "typegql-auth",
  "description": "Trying out TypeGraphQL",
  "author": "Khan Bifov",
  "license": "ISC",
  "version": "0.1.0",
  "homepage": "https://github.com/irestone/TypeGraphQL-auth#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/irestone/TypeGraphQL-auth.git"
  },
  "bugs": "https://github.com/irestone/TypeGraphQL-auth/issues",
  "main": "index.js",
  "scripts": {
    "start": "node bin/app",
    "start:dev": "node bin/dev",
    "start:dev:watch": "nodemon bin/dev -w build -q",
    "dev": "npm-run-all clean:dev build:dev -p start:dev:watch build:dev:watch",
    "build:dev": "npm run @rollup -- BUILD_ENV:development",
    "build:dev:watch": "npm run build:dev -- -w",
    "build:prod": "npm run @rollup -- BUILD_ENV:production",
    "clean": "run-p clean:*",
    "clean:dev": "rm -rf build",
    "clean:prod": "rm -rf dist",
    "prelint": "eslint --print-config . | eslint-config-prettier-check",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "@rollup": "rollup -c --environment"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/node": "^11.13.5",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.4.8",
    "core-js": "^3.0.1",
    "express": "^4.16.4",
    "graphql": "^14.2.1",
    "lodash": "^4.17.11",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.2",
    "type-graphql": "^0.17.1"
  }
}
